{
	"manifest_version": 3,
	"name": "Background Color Changer",
	"version": "1.0",
	"description": "Changes the background color of elements with the class 'fbb737a4'.",
	"permissions": ["activeTab", "scripting", "storage"],
	"action": {
		"default_popup": "popup.html",
		"default_icon": {
			"16": "images.png",
			"48": "images.png",
			"128": "images.png"
		}
	},
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["content.js"]
		}
	]
}
