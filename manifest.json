{
	"manifest_version": 3,
	"name": "DeepSeen",
	"version": "1.0",
	"description": "Find your propmts easily!!",
	"permissions": ["activeTab", "scripting", "storage"],
	"action": {
		"default_popup": "popup.html",
		"default_icon": {
			"16": "16.png",
			"48": "48.png",
			"128": "128.png"
		}
	},
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["content.js"]
		}
	]
}
