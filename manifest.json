{
	"manifest_version": 3,
	"name": "DeepSeen",
	"version": "1.0",
	"description": "Find your propmts easily!!",
	"permissions": ["activeTab", "scripting", "storage"],
	"action": {
		"default_popup": "popup.html",
		"default_icon": {
			"16": "images/16.png",
			"48": "images/48.png",
			"128": "images/128.png"
		}
	},
	"content_scripts": [
		{
			"matches": ["https://chat.deepseek.com/a/chat/s/*"],
			"js": ["content.js"]
		}
	]
}
